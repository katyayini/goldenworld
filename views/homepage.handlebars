<!-- Loop over each object in the projects array -->
{{#each blogs as |blog| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
        
      <a style="color: #b33756" href="/viewblog/{{blog.id}}">{{blog.title}}</a>
    </h2>
    <p>
     
      
      <span> {{blog.content}}</span>
    
    </p>
    <p>Posted by: {{blog.user.name}}</p>
  </div>
  
</div>
{{/each}}
